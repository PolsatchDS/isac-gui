,IAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OADA,EAEA;AAAA,QAAA,KAAA,EAAA,GAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAFA,EAGA;AAAA,QAAA,KAAA,EAAA,GAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAHA,CA3BA,CAgCA;AACA;;AAjCA,KAAA;AAmCA,GA5CA;AA6CA,EAAA,QAAA,kCACA,UAAA,CAAA,CACA,UADA,EAEA,aAFA,EAGA,eAHA,EAIA,mBAJA,EAKA,iBALA,EAMA,eANA,CAAA,CADA;AASA,IAAA,SATA,uBASA;AACA,aAAA,KAAA,iBAAA,CAAA,MAAA;AACA,KAXA;AAYA,IAAA,WAZA,yBAYA;AACA,aAAA,KAAA,iBAAA;AACA,KAdA;AAeA;AACA;AACA;AACA,IAAA,cAlBA,4BAmBA;AAAA;;AACA;AACA;AACA;AAEA,aAAA,KAAA,iBAAA,CAAA,MAAA,CAAA,UAAA,IAAA;AAAA,eAAA,IAAA,CAAA,KAAA,CAAA,WAAA,GAAA,KAAA,CAAA,KAAA,CAAA,MAAA,CAAA,WAAA,EAAA,CAAA;AAAA,OAAA,CAAA;AACA;AAzBA,IA7CA;AAwEA,EAAA,OAAA,EAAA,mBAAA;AACA,SAAA,KAAA,GAAA,KAAA,QAAA;AACA,GA1EA;AA2EA,EAAA,OAAA,kCAEA,UAAA,CAAA,CACA,SADA,EAEA,kBAFA,EAGA,iBAHA,EAIA,oBAJA,EAKA,qBALA,CAAA,CAFA;AASA,IAAA,SAAA,EAAA,mBAAA,MAAA,EAAA;AACA,WAAA,MAAA,GAAA,MAAA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,KAAA,MAAA,GAAA,MAAA;AAEA,KAbA;AAcA,IAAA,WAdA,uBAcA,IAdA,EAcA;AACA,WAAA,OAAA,CAAA,IAAA;AACA,WAAA,QAAA,CAAA,KAAA,WAAA,IAAA,CAAA,GAAA,2CAAA;AACA,QAAA,KAAA,EAAA,iBADA;AAEA,QAAA,OAAA,EAAA,SAFA;AAGA,QAAA,KAAA,EAAA,IAHA;AAIA,QAAA,aAAA,EAAA;AAJA,OAAA,EAFA,CAQA;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AAGA;AACA,KAlCA;AAmCA,IAAA,WAnCA,yBAmCA,CACA;AACA;AACA;AAGA,KAzCA;AA0CA,IAAA,oBA1CA,gCA0CA,YA1CA,EA0CA;AACA,MAAA,YAAA,GAAA,KAAA,KAAA,CAAA,WAAA;AACA,MAAA,YAAA,GAAA,YAAA,CACA,MADA,CACA,UAAA,OAAA;AAAA,eAAA,OAAA,CAAA,OAAA;AAAA,OADA,EAEA,GAFA,CAEA,UAAA,OAAA;AAAA,eAAA,OAAA,CAAA,KAAA;AAAA,OAFA,CAAA;AAGA,MAAA,OAAA,CAAA,GAAA,CAAA,YAAA;AACA,WAAA,gBAAA,CAAA,YAAA;AACA,WAAA,eAAA;AACA,KAlDA;AAmDA,IAAA,eAnDA,6BAmDA;AACA,WAAA,QAAA,CAAA,KAAA,OAAA,EAAA,CAAA;AACA,KArDA;AAsDA,IAAA,mBAtDA,+BAsDA,IAtDA,EAsDA;AACA,WAAA,eAAA,CAAA,IAAA;AACA,WAAA,eAAA;AACA,KAzDA;AA0DA,IAAA,sBA1DA,oCA0DA;AACA,WAAA,kBAAA;AACA,WAAA,eAAA;AACA,KA7DA;AA8DA,IAAA,uBA9DA,qCA8DA;AACA,WAAA,mBAAA;AACA,WAAA,eAAA;AACA,KAjEA;AAmEA,IAAA,gBAnEA,8BAmEA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,oBAAA;AACA,KArEA;AAsEA,IAAA,mBAtEA,+BAsEA,IAtEA,EAsEA;AACA,UAAA,IAAA,IAAA,KAAA,EAAA;AACA,QAAA,OAAA,CAAA,GAAA,CAAA,SAAA,EAAA,KAAA,kBAAA,EAAA;AACA;AACA,KA1EA;AA4EA,IAAA,QA5EA,sBA4EA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,kBAAA;AACA,KA9EA;AA+EA;AACA,IAAA,QAhFA,sBAgFA;AACA,WAAA,UAAA,GAAA,IAAA;AACA,KAlFA;AAmFA,IAAA,QAnFA,sBAmFA;AACA,WAAA,UAAA,GAAA,KAAA;AACA,KArFA;AAuFA;AACA,IAAA,QAxFA,oBAwFA,QAxFA,EAwFA,UAxFA,EAwFA;AACA,UAAA,YAAA,GAAA,KAAA,cAAA;AACA,WAAA,cAAA,GAAA,YAAA,CAAA,KAAA,CACA,UAAA,GAAA,QADA,EAEA,CAAA,UAAA,GAAA,CAAA,IAAA,QAFA,CAAA;AAIA,KA9FA;AA+FA,IAAA,aA/FA,yBA+FA,IA/FA,EA+FA;AACA,WAAA,QAAA,CAAA,KAAA,OAAA,EAAA,IAAA,GAAA,CAAA;AACA;AAjGA,IA3EA;AA8KA,EAAA,OA9KA,qBA8KA;AACA,SAAA,QAAA,CAAA,KAAA,OAAA,EAAA,CAAA;AACA;AAhLA,CAAA","sourcesContent":["<template>\r\n  <div class=\"main-content\">\r\n    <breadcumb :page=\"'Products'\" :folder=\"'Ecomerce'\" />\r\n    <b-row>\r\n      <b-col md=\"3\">\r\n        <button\r\n          @click=\"listView\"\r\n          class=\"btn btn-sm btn-rounded btn-outline-primary mb-3\"\r\n        >\r\n          <i class=\"i-Split-Four-Square-Window text-15\"></i>\r\n        </button>\r\n        <button\r\n          @click=\"gridView\"\r\n          class=\"btn btn-sm btn-rounded btn-outline-primary mb-3 ml-2\"\r\n        >\r\n          <i class=\"i-Align-Justify-All text-15\"></i>\r\n        </button>\r\n      </b-col>\r\n      <b-col md=\"9\" class=\"offset\">\r\n        <b-form-input v-model=\"search\" type=\"text\" placeholder=\"search....\"></b-form-input>\r\n        <!-- <vue-instantsearch> </vue-instantsearch> -->\r\n      </b-col>\r\n      <!-- <b-col md=\"3\">\r\n        <b-form-select\r\n          v-model=\"featured\"\r\n          :options=\"featuredOptions\"\r\n        ></b-form-select>\r\n      </b-col> -->\r\n    </b-row>\r\n    <b-row>\r\n      <b-col md=\"3\" class=\"mb-30\">\r\n        <b-card>\r\n          <b-form-group class>\r\n            <div class=\"text-center\">\r\n              <b-button block variant=\"primary mb-30\" @click=\"redirectCheckout\">\r\n                View Cart <b-badge variant=\"warning\" class=\"float-right pr-2 pl-2 text-11\">{{getAddToCarts.length}}</b-badge>\r\n              </b-button>\r\n            </div>\r\n            <label>\r\n              <h5 class=\"text-primary font-weight-600\">Price</h5>\r\n            </label>\r\n            <b-form-radio name=\"some-radios\" @change=\"ascendingOrderCartPage()\"\r\n              >Low to High</b-form-radio\r\n            >\r\n            <b-form-radio name=\"some-radios\" @change=\"descendingOrderCartPage()\"\r\n              >High to Low</b-form-radio\r\n            >\r\n          </b-form-group>\r\n          <b-form-group>\r\n            <label for=\"range-1\">\r\n              <h5 class=\"text-primary font-weight-600 mb-1 mt-3\">Category</h5>\r\n            </label>\r\n            <!-- <b-form-radio name=\"some-radios\" value=\"A\">Electronics</b-form-radio>\r\n            <b-form-radio name=\"some-radios\" value=\"B\">Mobile</b-form-radio>\r\n            <b-form-radio name=\"some-radios\" value=\"B\">Sports</b-form-radio>\r\n            <b-form-radio name=\"some-radios\" value=\"B\">Books</b-form-radio> -->\r\n            <b-form-radio\r\n              name=\"radio\"\r\n              :value=\"category\"\r\n              v-for=\"(category, index) in getCategoryItem\"\r\n              :key=\"index\"\r\n              @change=\"addCategoryItemPage(category)\"\r\n            >\r\n              {{ category }}\r\n            </b-form-radio>\r\n          </b-form-group>\r\n\r\n          <b-form-group>\r\n            <label for=\"range-1\">\r\n              <h5 class=\"text-primary font-weight-600 mb-1 mt-3\">Brand</h5>\r\n            </label>\r\n            <ul class=\"list-group\">\r\n              <li \r\n                v-for=\"(brand, index) in getBrandsItem\"\r\n                :key=\"index\"\r\n                class=\"list-group-item p-0 mb-2 border-0\"\r\n               \r\n              >\r\n                <div class=\"test d-flex\">\r\n                  <label class=\"checkbox checkbox-primary\">\r\n                    <input\r\n                      type=\"checkbox\"\r\n                      :value=\"brand\"\r\n                      ref=\"checkFilter\"\r\n                      @change=\"addBrandToFilterPage(brand)\"\r\n                    />\r\n\r\n                    <span class=\"checkmark\"></span>\r\n                  </label>\r\n                  {{ brand }}({{ brandsCount[brand] }})\r\n                </div>\r\n              </li>\r\n            </ul>\r\n          </b-form-group>\r\n        </b-card>\r\n      </b-col>\r\n      <b-col md=\"9\">\r\n        <!-- <ul v-for=\"(product, key) in getAddToCarts\" :key=\"key\">\r\n            <li><span>{{ product }}</span></li>\r\n          </ul> -->\r\n        <section class=\"product-cart\">\r\n          <div\r\n            ref=\"rowView\"\r\n            class=\"row\"\r\n            :class=\"{ 'list-grid': isListView, 'list-horizontal': !isListView }\"\r\n          >\r\n            <!-- loop LIST Shop -->\r\n\r\n            <div\r\n              :class=\"{ 'col-md-4': isListView, 'col-md-12': !isListView }\"\r\n              class=\"list-item\"\r\n              :key=\"index\"\r\n              v-for=\"(item, index) in paginatedItems\"\r\n            >\r\n              <div\r\n                class=\"card o-hidden mb-4 d-flex\"\r\n                :class=\"{ 'flex-column': isListView, 'flex-row': !isListView }\"\r\n              >\r\n                <div class=\"list-thumb d-flex\">\r\n                  <img alt :src=\"item.img\" />\r\n                </div>\r\n                <div\r\n                  class=\"flex-grow-1\"\r\n                  :class=\"{ 'd-bock': isListView, 'pl-2 d-flex': !isListView }\"\r\n                >\r\n                  <div\r\n                    class=\"card-body align-self-center d-flex flex-column justify-content-between align-items-lg-center\"\r\n                    :class=\"{ 'flex-lg-row': !isListView }\"\r\n                  >\r\n                    <a class=\"w-40 w-sm-100\" href>\r\n                      <div class=\"item-title\">{{ item.title }}</div>\r\n                    </a>\r\n                    <p class=\"text-muted text-small w-15 w-sm-100 mb-2\">\r\n                      {{ item.brand }}\r\n                    </p>\r\n\r\n                    <!-- star  -->\r\n                    <star-rating \r\n                        v-model=\"item.rating\"\r\n                        :star-size=\"15\" \r\n                        :show-rating=\"false\"\r\n                        @rating-selected =\"setRating\"\r\n                        class=\"mb-3\"\r\n                    ></star-rating>\r\n\r\n\r\n                    <div class=\"m-0 text-muted text-small w-15 w-sm-100\">\r\n                      <h6>$ {{ item.productPrice }}</h6>\r\n                    </div>\r\n                    <p\r\n                      class=\"m-0 text-muted text-small w-15 w-sm-100 d-none d-lg-block item-badges\"\r\n                    >\r\n                      <span :class=\"item.badgeColor\" class=\"badge\">{{\r\n                        item.discount\r\n                      }}</span>\r\n                    </p>\r\n\r\n                    <div class=\"d-flex justify-content-between\">\r\n                      <b-button\r\n                        size=\"sm\"\r\n                        @click=\"addCartPage(item)\"\r\n                        variant=\"primary\"\r\n                        class=\"mr-2\"\r\n                        >Add Cart</b-button\r\n                      >\r\n                      <b-button\r\n                        size=\"sm\"\r\n                        class=\"ul-product\"\r\n                        @click=\"viewProduct\"\r\n                        variant=\"outline-primary\"\r\n                        >View Cart</b-button\r\n                      >\r\n                    </div>\r\n                  </div>\r\n                </div>\r\n              </div>\r\n            </div>\r\n            <!-- </transition-group>\r\n            -->\r\n          </div>\r\n        </section>\r\n      </b-col>\r\n    </b-row>\r\n    <b-row>\r\n      <b-col md=\"12\" class=\"mt-4\">\r\n        <b-pagination\r\n          @change=\"onPageChanged\"\r\n          :total-rows=\"totalRows\"\r\n          :per-page=\"perPage\"\r\n          v-model=\"currentPage\"\r\n          class=\"my-0 gull-pagination align-items-center\"\r\n          align=\"center\"\r\n          first-text\r\n          last-text\r\n        >\r\n          <p class=\"list-arrow m-0\" slot=\"prev-text\">\r\n            <i class=\"i-Arrow-Left text-40\"></i>\r\n          </p>\r\n          <p class=\"list-arrow m-0\" slot=\"next-text\">\r\n            <i class=\"i-Arrow-Right text-40\"></i>\r\n          </p>\r\n        </b-pagination>\r\n      </b-col>\r\n    </b-row>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters, mapActions } from \"vuex\";\r\nimport StarRating from 'vue-star-rating'\r\n\r\nexport default {\r\n  metaInfo: {\r\n    // if no subcomponents specify a metaInfo.title, this title will be used\r\n    title: \"List Table\"\r\n  },\r\n  components: {\r\n    StarRating\r\n  },\r\n  data() {\r\n    return {\r\n      rating: 4,\r\n      search:\"\",\r\n      isProduct:true,\r\n      filterBrand: \"\",\r\n      priceValue: 500,\r\n      selectedFilter: \"\",\r\n      selectPriceRange: \"\",\r\n      priceRange: 1,\r\n      value: 1,\r\n      items: \"\",\r\n      paginatedItems: \"\",\r\n      currentPage: 1,\r\n      perPage: 9,\r\n      // totalRows: '',\r\n      isFirst: false,\r\n      // button\r\n      isListView: true,\r\n      // isGridVIew:false,\r\n      selected: [], // Must be an array reference!\r\n      // options: [\r\n      //   { text: 'Orange', value: 'orange' },\r\n      //   { text: 'Apple', value: 'apple' },\r\n      //   { text: 'Pineapple', value: 'pineapple' },\r\n      //   { text: 'Grape', value: 'grape' }\r\n      // ],\r\n      featured: null,\r\n      featuredOptions: [\r\n        { value: null, text: \"Featured\" },\r\n        { value: \"a\", text: \"Highest Price\" },\r\n        { value: \"b\", text: \"Lowest Price\" }\r\n      ]\r\n      // brandsCount: this.$store.getters.brandsCount,\r\n      // brands: this.$store.state.brands\r\n    };\r\n  },\r\n  computed: {\r\n    ...mapGetters([\r\n      \"getItems\",\r\n      \"brandsCount\",\r\n      \"getBrandsItem\",\r\n      \"getFilterProducts\",\r\n      \"getCategoryItem\",\r\n      \"getAddToCarts\"\r\n    ]),\r\n    totalRows() {\r\n      return this.getFilterProducts.length;\r\n    },\r\n    filterItems() {\r\n      return this.getFilterProducts;\r\n    },\r\n    // searchProducts(){\r\n    //   return this.getFilterProducts.filter(item => item.title.toLowerCase().match(this.search.toLowerCase()))\r\n    // }\r\n    filterProducts()\r\n        {\r\n            // return this.getItems.filter((filterSearch) => {\r\n            //     return filterSearch.toLowerCase().match(this.search.toLowerCase());\r\n            // }); \r\n            \r\n            return this.getFilterProducts.filter(item => item.title.toLowerCase().match(this.search.toLowerCase()))\r\n        }\r\n  },\r\n  created: function() {\r\n    this.items = this.getItems;\r\n  },\r\n  methods: {\r\n    \r\n    ...mapActions([\r\n      \"addCart\",\r\n      \"addBrandToFilter\",\r\n      \"addCategoryItem\",\r\n      \"ascendingOrderCart\",\r\n      \"descendingOrderCart\"\r\n    ]),\r\n    setRating: function(rating){\r\n      this.rating= rating;\r\n      console.log(this.rating= rating);\r\n      \r\n    },\r\n    addCartPage(item){\r\n      this.addCart(item)\r\n      this.$bvToast.toast(`${ item.qty } item added to the cart successfully`, {\r\n          title: 'Click View Cart',\r\n          variant: 'primary',\r\n          solid: true,\r\n          autoHideDelay: 700\r\n        })\r\n      // this.$swal({\r\n        \r\n      //   toast: true,\r\n      //   position: \"top-end\",\r\n      //   showConfirmButton: false,\r\n      //   timer: 1000,\r\n      //   type: \"success\",\r\n      //   title: `${ item.qty } item added to the cart successfully`\r\n      // });\r\n\r\n      \r\n      // this.$router.push(\"/app/apps/checkout\");\r\n    },\r\n    viewProduct(){\r\n      // this.$router.push(\"product-detail\")\r\n      // console.log(this.getFilterProducts.filter(item => item.title.toLowerCase().match(this.search.toLowerCase())));\r\n      // console.log(this.search);\r\n      \r\n      \r\n    },\r\n    addBrandToFilterPage(checkedArray) {\r\n      checkedArray = this.$refs.checkFilter;\r\n      checkedArray = checkedArray\r\n        .filter(element => element.checked)\r\n        .map(element => element.value);\r\n      console.log(checkedArray);\r\n      this.addBrandToFilter(checkedArray);\r\n      this.paginatePerPage();\r\n    },\r\n    paginatePerPage() {\r\n      this.paginate(this.perPage, 0);\r\n    },\r\n    addCategoryItemPage(data) {\r\n      this.addCategoryItem(data);\r\n      this.paginatePerPage();\r\n    },\r\n    ascendingOrderCartPage() {\r\n      this.ascendingOrderCart();\r\n      this.paginatePerPage();\r\n    },\r\n    descendingOrderCartPage() {\r\n      this.descendingOrderCart();\r\n      this.paginatePerPage();\r\n    },\r\n  \r\n    redirectCheckout(){\r\n      this.$router.push(\"/app/apps/checkout\")\r\n    },\r\n    onRadioButtonChange(data) {\r\n      if (data == \"asc\") {\r\n        console.log(\"Working\", this.ascendingOrderCart());\r\n      }\r\n    },\r\n\r\n    viewCart() {\r\n      this.$router.push(\"./product-detail\");\r\n    },\r\n    // button\r\n    listView() {\r\n      this.isListView = true;\r\n    },\r\n    gridView() {\r\n      this.isListView = false;\r\n    },\r\n\r\n    // end-button\r\n    paginate(pageSize, pageNumber) {\r\n      let itemsToParse = this.filterProducts;\r\n      this.paginatedItems = itemsToParse.slice(\r\n        pageNumber * pageSize,\r\n        (pageNumber + 1) * pageSize\r\n      );\r\n    },\r\n    onPageChanged(page) {\r\n      this.paginate(this.perPage, page - 1);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.paginate(this.perPage, 0);\r\n  }\r\n};\r\n</script>\r\n"],"sourceRoot":"src/views/app/apps"}]}